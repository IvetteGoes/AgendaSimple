<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.saraivette.agendasimple" >

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PerfildeUsuarioListActivity"
            android:label="@string/title_perfildeusuario_list" >
        </activity>
        <activity
            android:name=".PerfildeUsuarioDetailActivity"
            android:label="@string/title_perfildeusuario_detail"
            android:parentActivityName=".PerfildeUsuarioListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.saraivette.agendasimple.PerfildeUsuarioListActivity" />
        </activity>
    </application>

</manifest>
